@model EntityFrameWork_MVCDemo.Models.Batch

@{
    ViewData["Title"] = "Search";
}

<h1>Search</h1>
@{ if (ViewBag.msg == null)
    {
        <div>
            <h4>Batch</h4>
            <hr />
            <dl class="row">
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.BatchCode)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.BatchCode)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.Course)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.Course)
                </dd>
            </dl>
        </div>
        <div>
